{
    "parserOptions": {
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
        "valid-jsdoc": "off",
        "jsdoc/check-tag-names": "off",
        "no-console": "off",
        "semi": ["warn", "always"],
        "quotes": ["warn", "double"],
        "arrow-parens": ["warn", "always"],
        "no-unused-vars": "warn",
        "import/prefer-default-export": "off"
    },
    "extends": "distube"
}
