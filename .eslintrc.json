{
	"root": true,
	"env": {
	  "es2021": true,
	  "node": true
	},
	"extends": [
	  "eslint:recommended",
	  "prettier"
	],
	"parserOptions": {
	  "ecmaVersion": "latest",
	  "sourceType": "module"
	},
	"rules": {
	  "semi": ["warn", "always"],
	  "quotes": ["warn", "double"],
	  "arrow-parens": ["warn", "always"],
	  "no-unused-vars": "warn",
	  "no-console": "off",
	  "import/prefer-default-export": "off"
	}
  }